{
  "name": "apollo-bookstore-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "gql:schema": "apollo service:download -c apollo.config.js",
    "gql:types": "apollo client:codegen -c apollo.config.js --localSchemaFile=schema.json --target=typescript --globalTypesFile=src/graphql-types.ts",
    "format": "prettier --write 'src/**/*.ts*'"
  },
  "dependencies": {
    "@material-ui/core": "^3.8.3",
    "@material-ui/icons": "^3.0.2",
    "apollo-cache-inmemory": "^1.3.12",
    "apollo-client": "^2.4.8",
    "apollo-link": "^1.2.6",
    "apollo-link-error": "^1.1.5",
    "apollo-link-http": "^1.5.9",
    "apollo-link-ws": "^1.0.12",
    "classnames": "^2.2.6",
    "formik": "^1.4.2",
    "global": "^4.3.2",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "history": "^4.7.2",
    "mobx": "^4.8.0",
    "mobx-react": "^5.4.3",
    "mobx-state-router": "^4.0.3",
    "react": "^16.7.0",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.7.0",
    "react-router-dom": "^5.0.0",
    "react-transition-group": "^2.9.0",
    "subscriptions-transport-ws": "^0.9.15",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/graphql": "^14.0.4",
    "@types/history": "^4.7.2",
    "@types/jest": "23.3.12",
    "@types/node": "10.12.18",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "apollo": "^2.8.1",
    "prettier": "^1.15.3",
    "react-scripts": "^2.1.8",
    "typescript": "3.2.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
